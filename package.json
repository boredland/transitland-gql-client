{
  "name": "@ioki/transitland-gql-client",
  "version": "2022.6.5",
  "description": "gql client for transitland",
  "repository": "git@github.com:boredland/transitland-gql-client.git",
  "author": "Jonas Strassel <info@jonas-strassel.de>",
  "license": "MIT",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/esm/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "generate": "yarn run-s generate:* --print-label",
    "generate:0-download": "curl -s https://raw.githubusercontent.com/interline-io/transitland-server/main/schema.graphqls > ./landing/schema.graphqls",
    "generate:1-client": "yarn genql --schema landing/schema.graphqls --output src/_client --sort --esm-and-cjs",
    "generate:2-0-bundle-cjs": "tsup src/index.ts --format cjs --dts --clean",
    "generate:2-1-bundle-esm": "tsup src/index.ts --format esm --clean --out-dir dist/esm",
    "generate:3-cp-schema": "cp src/_client/schema.graphql dist/schema.graphql",
    "prepublishOnly": "yarn generate",
    "release": "release-it",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@csmith/release-it-calver-plugin": "2022.5.13",
    "@genql/cli": "2.10.0",
    "@release-it/conventional-changelog": "5.0.0",
    "@types/jest": "28.1.0",
    "dotenv": "16.0.1",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "npm-run-all": "4.1.5",
    "release-it": "15.0.0",
    "ts-jest": "28.0.3",
    "ts-node": "10.8.0",
    "tsup": "6.0.1",
    "typescript": "4.7.2",
    "utility-types": "3.10.0"
  },
  "dependencies": {
    "@genql/runtime": "2.10.0"
  },
  "peerDependencies": {
    "graphql": "16.5.0"
  }
}
