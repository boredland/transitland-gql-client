{
  "name": "@ioki/transitland-gql-client",
  "version": "2022.6.17",
  "description": "gql client for transitland",
  "repository": "git@github.com:ioki-mobility/transitland-gql-client.git",
  "author": "Jonas Strassel <jonas.strassel@ioki.com>",
  "license": "MIT",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "generate": "yarn run-s generate:* --print-label",
    "generate:0-download": "curl -s https://raw.githubusercontent.com/interline-io/transitland-server/main/schema.graphqls > ./landing/schema.graphqls",
    "generate:1-client": "yarn genql --schema landing/schema.graphqls --output src/_client --esm --sort --clean",
    "generate:2-bundle": "tsup src/index.ts --format cjs,esm --dts --clean --minify",
    "generate:3-cp-schema": "cp src/_client/schema.graphql dist/schema.graphql",
    "postpublish": "git checkout HEAD README.md",
    "prepublishOnly": "node npmReadme.js",
    "release": "release-it",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@csmith/release-it-calver-plugin": "2022.5.13",
    "@genql/cli": "2.10.0",
    "@graphql-tools/mock": "8.6.12",
    "@graphql-tools/schema": "8.3.14",
    "@release-it/conventional-changelog": "5.0.0",
    "@types/chance": "1.1.3",
    "@types/connect": "3.4.35",
    "@types/jest": "28.1.1",
    "chance": "1.1.8",
    "connect": "3.7.0",
    "dotenv": "16.0.1",
    "express-graphql": "0.12.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "npm-run-all": "4.1.5",
    "release-it": "15.0.0",
    "ts-jest": "28.0.4",
    "ts-node": "10.8.1",
    "tsup": "6.1.2",
    "typescript": "4.7.3",
    "utility-types": "3.10.0"
  },
  "dependencies": {
    "@genql/runtime": "2.10.0"
  },
  "peerDependencies": {
    "graphql": "*"
  }
}
